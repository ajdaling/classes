#include "User.h"

using namespace std;
namespace caps{



User::User(string name) {
	user_name = name;

}

User::~User() {
	// TODO Auto-generated destructor stub
}

string User::getName(){
	return(user_name);
}


void User::saveScore(round_score){
	score_history.push_back(round_score&);
}
}
